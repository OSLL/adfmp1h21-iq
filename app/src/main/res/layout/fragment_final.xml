<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".FinalFragment" android:id="@+id/frameLayout">
    <Button
            android:text="@string/button_rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button_rate"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="196dp"
            app:layout_constraintHorizontal_bias="0.501"/>
    <Button
            android:text="@string/button_share"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button_share"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.501" app:layout_constraintTop_toBottomOf="@+id/button_rate"
            android:layout_marginTop="36dp"/>
    <Button
            android:text="@string/button_answers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button_answers"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_share" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.501" app:layout_constraintVertical_bias="0.03"
            android:layout_marginTop="32dp"/>
    <Button
            android:text="@string/button_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button_home"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.894"
            android:layout_marginBottom="40dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>